<h1>
  <%= "Автобусы #{@from.name} – #{@to.name}" %>
</h1>
<h2>
  <%= "В расписании #{@trips.count} рейсов" %>
</h2>


<% @trips.each do |trip| %>
  <ul>
    <%= render "trip", trip: trip %>
    <%= render "services", services: trip.bus.services %>
  </ul>
  <%= render "delimiter" %>
<% end %>

